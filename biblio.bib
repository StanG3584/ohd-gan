@Article{Ye2019,
  author    = {Ye, F. and Zhu, F. and Fu, Y. and Shen, B.},
  journal   = {IEEE Access},
  title     = {{ECG} {Generation} {With} {Sequence} {Generative} {Adversarial} {Nets} {Optimized} by {Policy} {Gradient}},
