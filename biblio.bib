file = {:C$\backslash$:/Users/jerem/Google Drive/Mendeley library/Harada, Hayashi, Uchida/2019/Harada, Hayashi, Uchida - Biosignal Generation and Latent Variable Analysis with Recurrent Generative Adversarial Networks - 2019.pdf:pdf},
issn = {21693536},
journal = {IEEE Access},
keywords = {Biosignal generative model,generative adversarial networks,latent variable analysis},
mendeley-groups = {OHD-GAN-misc,Phylogenetics},
month = {may},
pages = {144292--144302},
publisher = {Institute of Electrical and Electronics Engineers Inc.},
