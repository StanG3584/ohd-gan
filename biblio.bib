keywords = {generative adversarial networks,privacy,synthetic data generation,time series},
mendeley-groups = {Privacy,GAN,OHD-GAN-misc,Synthetic data},
month = {sep},
title = {{Generating High-fidelity, Synthetic Time Series Datasets with DoppelGANger}},
url = {http://arxiv.org/abs/1909.13403},
year = {2019}
}

