@article{Lipton2016,
abstract = {We demonstrate a simple strategy to cope with missing data in sequential
      inputs, addressing the task of multilabel classification of diagnoses
      given clinical time series. Collected from the intensive care unit (ICU)
      of a major urban medical center, our data consists of multivariate time
      series of observations. The data is irregularly sampled, leading to
      missingness patterns in re-sampled sequences. In this work, we show the
      remarkable ability of RNNs to make effective use of binary indicators to
      directly model missing data, improving AUC and F1significantly. However,
      while RNNs can learn arbitrary functions of the missing data and
      observations, linear models can only learn substitution values. For linear
      models and MLPs, we show an alternative strategy to capture this signal.
      Additionally, we evaluate LSTMs, MLPs, and linear models trained on
      missingness patterns only, showing that for several diseases, what tests
      are run can be more predictive than the results themselves.},
address = {Children's Hospital LA, Los Angeles, CA, USA},
author = {Lipton, Zachary C and Kale, David and Wetzel, Randall},
keywords = {Unsorted},
mendeley-groups = {Phylogenetics,OHD-GAN-misc},
pages = {253--270},
publisher = {PMLR},
title = {{Directly Modeling Missing Data in Sequences with RNNs: Improved
      Classification of Clinical Time Series}},
url = {http://proceedings.mlr.press/v56/Lipton16.html All Papers/Other/Lipton et al. 2016 - Directly Modeling Missing Data in Sequences with RNNs - Improved Classification of Clinical Time Series.pdf},
volume = {56},
year = {2016}
