issn = {00189448},
journal = {IEEE Transactions on Information Theory},
keywords = {Bhattacharyya distance,Kullback-Leibler divergence,Pythagorean inequality,Renyi divergence,information divergence,$\alpha$-divergence},
number = {7},
pages = {3797--3820},
title = {{R{\'{e}}nyi divergence and kullback-leibler divergence}},
