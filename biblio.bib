}


@ARTICLE{Kansal2018-dx,
  title       = "Alzheimer's disease {Archimedes} condition-event simulator:
                 {Development} and validation",
  author      = "Kansal, Anuraag R and Tafazzoli, Ali and Ishak, K Jack and
                 Krotneva, Stanmira",
  affiliation = "Modeling and Simulation, Evidera, Bethesda, MD, USA. Modeling
                 and Simulation, Evidera, St-Laurent, Quebec, Canada.",
  abstract    = "Introduction: Several advances have been made in Alzheimer's
                 Disease (AD) modeling, however, there remains a need for a
